let i = 1;
while (i <= 5) {
  console.log(i);
  i++;
}

let input;
do {
  input = prompt("Enter 1 to stop:");
} while (input != 1);

let sum = 0;
for (let j = 1; j <= 5; j++) {
  sum += j;
}
console.log("Sum:", sum);

for (let x = 1; x <= 3; x++) {
  for (let y = 1; y <= 3; y++) {
    console.log(`${x} x ${y} = ${x * y}`);
  }
}
