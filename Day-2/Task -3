for (let i = 2; i <= 10; i++) {
  let prime = true;
  for (let j = 2; j < i; j++) {
    if (i % j === 0) {
      prime = false;
      break;
    }
  }
  if (prime) {
    console.log("First prime:", i);
    break;
  }
}

for (let k = 1; k <= 10; k++) {
  if (k % 2 === 0) continue;
  console.log(k);
}
